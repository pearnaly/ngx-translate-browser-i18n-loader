    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "rimraf": "^3.0.2",
    "rxjs": "^7.0.0",
    "zone.js": "^0.11.4",
    "typescript": "^4.2.4"
  }
}
